# Alchemy RPC URL for Sepolia testnet
# Get from: https://www.alchemy.com/ (create app, select Sepolia)
ALCHEMY_SEPOLIA_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY

# Deployer wallet private key (NEVER COMMIT THIS TO GIT!)
# Export from MetaMask: Account Details > Export Private Key
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Etherscan API key for contract verification
# Get from: https://etherscan.io/myapikey
ETHERSCAN_API_KEY=your_etherscan_api_key

# Deployed contract address (will be filled after deployment)
CONTRACT_ADDRESS=
